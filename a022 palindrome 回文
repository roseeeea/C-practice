#include<iostream>
using namespace std;

int main(){
	string s;
	int sum1=0;
	int sum2=0;
	
	while(cin>>s){
		 string B =""; 
		for(int i=s.length()-1; i>=0; i--){
			B= B + s[i]; 	//=B.append(1, s[i]);
		}
		
		if(B==s) {
			cout<<"yes" <<endl;
		}
		else{
			cout<<"no"<<endl;
		}
		
	}
	
	return 0;
}
	
