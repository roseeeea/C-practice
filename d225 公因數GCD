//https://zerojudge.tw/ShowProblem?problemid=d255


#include<iostream>
using namespace std;

int GCD(int x, int y){		// 求最大公因數 
	if(x%y != 0){
		return GCD(y, x%y); 
	}	
	else{
		return y;
	}
}

int main(){
	int N;
	
	while(cin >> N && N!=0){
		int G=0;
		for(int i=1;i<N;i++){
			for(int j=i+1;j<=N;j++){
    			G+=GCD(i,j);
			}
		}
		cout << G << endl;
	}
	
	return 0;
}
