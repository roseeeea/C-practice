			mid = (low + upper)/2;
			
			if (A[mid] < x){
				low = mid +1;
			}
			else if (A[mid] > x){
				upper = mid -1;
			}
			else{
				find = 1;
				break;
			}
		}
			if(find ==1 ){
				cout << mid+1 << endl;
			}
