*/
#include<iostream>
using namespace std;

int main(){
	int n, k;
	int arri[100000];
	int arrx[100000];
	
	while(cin >> n >> k && n,k > 0){
		
		for(int i=0; i<n; i++){
			cin >> arri[i];
		}
		for(int i=0; i<n; i++){
			cin >> arrx[i]; 
		}
		
		
		int low = 0;
		int upper = n-1;
		int mid = (low + upper)/2;
		
		
			for (int i=0; i<k; i++){
				
				while( low <= upper)
				if(arri[mid] < arrx[i] ){
					low = mid +1;
				}
				else if(arri[mid] > arrx[i]){
					upper = mid-1;
				}
				else if (arri[mid] == arrx[i]) {
					cout << mid << endl;
				}
				else{
					cout << 0 << endl;
				}
				
			}
		
	}
	
	return 0;
}
