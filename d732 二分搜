// https://zerojudge.tw/ShowProblem?problemid=d732
#include<iostream>
using namespace std;

int main(){
	int n,k;
	int A[100000];
	int x;
	
	while(cin >> n>> k){
		for (int i=0; i<n; i++){
			cin >> A[i];
		}
		for (int i=0; i<k; i++){
			cin >> x;
		
		
		int low=0;
		int upper= n-1;
		int mid;
		int find =0;
		
		while(low <= upper){
			mid = (low + upper)/2;
			
			if (A[mid] < x){
				low = mid +1;
			}
			else if (A[mid] > x){
				upper = mid -1;
			}
			else{
				find = 1;
				break;
			}
		}
			if(find ==1 ){
				cout << mid+1 << endl;
			}
			else{
				cout << 0 << endl;
			}
		
		}
	}
		return 0;
	}
